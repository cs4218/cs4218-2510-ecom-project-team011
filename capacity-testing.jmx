          <JSR223Assertion guiclass="TestBeanGUI" testclass="JSR223Assertion" testname="Error Rate Assertion" enabled="true">
            <stringProp name="cacheKey">true</stringProp>
            <stringProp name="filename"></stringProp>
            <stringProp name="parameters"></stringProp>
            <stringProp name="script">import java.util.concurrent.atomic.AtomicLong;
import java.util.concurrent.atomic.AtomicInteger;
import java.util.BitSet;

double maxErrorRate = 0.1;
int windowSize = 5000;

// Initialize in props
if (!props.containsKey(&quot;errorBitSet&quot;)) {
    synchronized(props) {
        if (!props.containsKey(&quot;errorBitSet&quot;)) {
            props.put(&quot;errorBitSet&quot;, new BitSet(windowSize));
            props.put(&quot;errorCount&quot;, new AtomicLong(0));
            props.put(&quot;currentIndex&quot;, new AtomicInteger(0));
            props.put(&quot;totalSamplesProcessed&quot;, new AtomicLong(0));
        }
    }
}

BitSet errorBits = props.get(&quot;errorBitSet&quot;);
AtomicLong errorCount = props.get(&quot;errorCount&quot;);
AtomicInteger currentIndex = props.get(&quot;currentIndex&quot;);
AtomicLong totalProcessed = props.get(&quot;totalSamplesProcessed&quot;);

// Get current position in circular buffer
int index = currentIndex.getAndIncrement() % windowSize;
long total = totalProcessed.incrementAndGet();

// Check if we&apos;re overwriting an old error
boolean oldWasError = errorBits.get(index);

// Determine if current sample is an error
boolean isError = !prev.isSuccessful();

// Update the bit
if (isError) {
    errorBits.set(index);
    if (!oldWasError) {
        errorCount.incrementAndGet();
    }
} else {
    errorBits.clear(index);
    if (oldWasError) {
        errorCount.decrementAndGet();
    }
}

// Calculate error rate (only use full window after first 10k samples)
int samplesInWindow = (int) Math.min(total, windowSize);
long errors = errorCount.get();
double errorRate = (errors / (double) samplesInWindow) * 100.0;

// Check threshold
if (errorRate &gt; maxErrorRate) {
    String msg = String.format(&quot;ERROR RATE EXCEEDED: %.2f%% &gt; %.2f%% (Errors: %d/%d in window)&quot;, 
                               errorRate, maxErrorRate, errors, samplesInWindow);
    log.error(&quot;*** &quot; + msg + &quot; ***&quot;);
    prev.setSuccessful(false);
    prev.setResponseMessage(msg);
    props.put(&quot;stopTest&quot;, &quot;true&quot;);
} else {
    prev.setResponseMessage(String.format(&quot;Error rate: %.2f%% (E:%d/W:%d)&quot;, 
                                         errorRate, errors, samplesInWindow));
}</stringProp>
            <stringProp name="scriptLanguage">groovy</stringProp>
          </JSR223Assertion>
