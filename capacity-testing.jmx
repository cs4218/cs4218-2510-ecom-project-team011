          <JSR223Assertion guiclass="TestBeanGUI" testclass="JSR223Assertion" testname="Throughput Assertion" enabled="true">
            <stringProp name="cacheKey">true</stringProp>
            <stringProp name="filename"></stringProp>
            <stringProp name="parameters"></stringProp>
            <stringProp name="script">import java.util.concurrent.ConcurrentLinkedQueue;
import java.util.concurrent.atomic.AtomicLong;

double minThroughput = 6000.0;
long windowSize = 1000; // 1 second window in ms
long warmupPeriod = 300000; // 3 min warmup

long currentTime = System.currentTimeMillis();

// Initialize directly in props
if (!props.containsKey(&quot;throughputTimestamps&quot;)) {
    synchronized(props) {
        if (!props.containsKey(&quot;throughputTimestamps&quot;)) {
            props.put(&quot;throughputTimestamps&quot;, new ConcurrentLinkedQueue());
            props.put(&quot;throughputStartTime&quot;, new AtomicLong(currentTime));
        }
    }
}

ConcurrentLinkedQueue timestamps = props.get(&quot;throughputTimestamps&quot;);
AtomicLong testStartTime = props.get(&quot;throughputStartTime&quot;);

// Add current sample
timestamps.add(currentTime);

// Clean up old timestamps
long cutoffTime = currentTime - windowSize;
while (timestamps.peek() != null &amp;&amp; timestamps.peek() &lt; cutoffTime) {
    timestamps.poll();
}

// Calculate throughput
int sampleCount = timestamps.size();
double throughput = sampleCount / (windowSize / 1000.0);

// Check threshold only after warmup
long testRuntime = currentTime - testStartTime.get();
if (testRuntime &gt;= warmupPeriod &amp;&amp; throughput &lt; minThroughput) {
    String msg = String.format(&quot;THROUGHPUT DEGRADED: %.2f req/s &lt; %.2f req/s (samples: %d)&quot;, 
                               throughput, minThroughput, sampleCount);
    log.error(&quot;*** &quot; + msg + &quot; ***&quot;);
    prev.setSuccessful(false);
    prev.setResponseMessage(msg);
    props.put(&quot;stopTest&quot;, &quot;true&quot;);
}</stringProp>
            <stringProp name="scriptLanguage">groovy</stringProp>
