          <JSR223Assertion guiclass="TestBeanGUI" testclass="JSR223Assertion" testname="Duration Assertion Flag Check">
            <stringProp name="cacheKey">true</stringProp>
            <stringProp name="filename"></stringProp>
            <stringProp name="parameters"></stringProp>
            <stringProp name="script">// Check if Duration Assertion failed and set stop flag
// This runs after the Duration Assertion, so we can check if it failed
if (!prev.isSuccessful()) {
    // Check if failure was due to Duration Assertion
    String responseMessage = prev.getResponseMessage();
    if (responseMessage != null &amp;&amp; responseMessage.contains(&quot;lasted too long&quot;)) {
        log.error(&quot;*** DURATION ASSERTION FAILED: Response time exceeded threshold ***&quot;);
        props.put(&quot;stopTest&quot;, &quot;true&quot;);
    }
}</stringProp>
